{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug OS Project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/os",
      "args": ["mem1"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build-os",
      "miDebuggerPath": "/usr/bin/gdb",
      "logging": {
        "trace": true,
        "engineLogging": true,
        "programOutput": true
      }
    }
  ]
}